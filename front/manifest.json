{
  "name": "Ad-blocker",
  "manifest_version": 3,
  "description": "Block ADs on every website!!",
  "version": "1.0",

  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "view/popup.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
